// Prisma schema file

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url = "file:./dev.db"
  
}

model Paste {
  id         String   @id @default(uuid())
  content    String
  expiresAt  DateTime?
  maxViews   Int?
  views      Int      @default(0)
  createdAt  DateTime @default(now())
}
